<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:clickable="true"
    android:focusable="true"
    tools:parentTag="android.widget.RelativeLayout"
    android:layout_height="wrap_content">

    <com.cisco.veop.sf_ui.ui_configuration.UiConfigTextView
        android:id="@+id/buttonText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:visibility="gone"
        />

    <com.cisco.veop.sf_ui.ui_configuration.UiConfigTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_trickmode_icon_gylph"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/buttonText"
        />

    <com.cisco.veop.sf_ui.ui_configuration.UiConfigTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/inner_trickmode_icon_text"
        android:layout_centerInParent="true"
        />

    <com.cisco.veop.sf_ui.ui_configuration.UiConfigTextView
        android:id="@+id/buttonDescription"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_trickmode_icon_gylph"
        android:layout_centerHorizontal="true"
        android:visibility="gone" />

</merge>
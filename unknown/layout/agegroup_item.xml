<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/profile_age_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        ></TextView>

    <TextView
        android:id="@+id/profile_age_description"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/profile_age_name"

        app:layout_constraintLeft_toLeftOf="parent"
        ></TextView>

    <com.cisco.veop.sf_ui.ui_configuration.UiConfigTextView
        android:id="@+id/select_age_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"


        ></com.cisco.veop.sf_ui.ui_configuration.UiConfigTextView>

    <View
        android:id="@+id/view_divider_profile_name"
        android:layout_width="match_parent"
        android:layout_height="1dp"

        android:background="#33FFFFFF"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_age_description" />


</androidx.constraintlayout.widget.ConstraintLayout>